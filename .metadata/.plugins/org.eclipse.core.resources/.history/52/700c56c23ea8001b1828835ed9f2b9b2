package ders2ödev;

import java.util.Iterator;

public class Main {

	public static void main(String[] args) {
		
		CourseManager coursemanager1 = new CourseManager(1,"Engin Demiroğ",21.00);
		CourseManager coursemanager2 = new CourseManager(1,"Emrah yüksel",22.00);
		CourseManager coursemanager3 = new CourseManager(1,"Emre Şahin",23.00);
		
		CourseManager [] courses = {coursemanager1,coursemanager2,coursemanager3};
		
		for (CourseManager courseManager : courses) {
			System.out.printf(courseManager.ders_no,courseManager.egitmen,courseManager.saati );
		}
		
	}

}
