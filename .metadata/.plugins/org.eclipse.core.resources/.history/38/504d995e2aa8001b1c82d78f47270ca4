package oopIntro;

public class Main {

	public static void main(String[] args) {
	
		Product product1 = new Product(); /*Classý oluþturmak için bunu yapmamýz lazým*/
		product1.id=1;
		product1.name="Lenovo v14";
		product1.unitPrice = 12.400;
		product1.detail = "16 GB Ram";
		

		Product product2 = new Product(); /*Classý oluþturmak için bunu yapmamýz lazým*/
		product1.id=2;
		product1.name="Lenovo v15";
		product1.unitPrice = 14.400;
		product1.detail = "32 GB Ram";
		

		Product product3 = new Product(); /*Classý oluþturmak için bunu yapmamýz lazým*/
		product1.id=1;
		product1.name="HP 5";
		product1.unitPrice = 12.000;
		product1.detail = "8 GB Ram";
		
		
		Product[] products = {product1,product2,product3};
		
		for (Product product : products) {
			System.out.println(product.id);
		}
		
		
		
		System.out.println(products.length);
		
		
	
		

	}

}
